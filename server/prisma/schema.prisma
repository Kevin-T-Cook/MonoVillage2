generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Boat {
  id        Int     @id @default(autoincrement())
  boatType  String?
  maxGuests String?
  halfDay   String?
  fullDay   String?
}

model Cabin {
  id        Int     @id @default(autoincrement())
  cabinName String?
  occupancy String?
  amenities String?
  price     String?
  cabinType String?
}

model User {
  id       Int     @id @default(autoincrement())
  fName    String?
  lName    String?
  email    String?
  password String?
  phone    String?
  street   String?
  city     String?
  state    String?
  zip      String?
  suite    String?
}

model Storage {
  id          Int     @id @default(autoincrement())
  storageType String?
  unitSize    String?
  price       String?
}
